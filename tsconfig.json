{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@typings/*": ["./src/typings/*"],
      "@config/*": ["./src/config/*"],
      "@store/*": ["./src/store/*"],
      "@utils/*": ["./src/utils/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@assets/*": ["./assets/*"]
    },
    "typeRoots": ["../node_modules/@types", "../@types"]
  }
}
